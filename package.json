{
  "name": "test-mithram",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "local-node": "cd src/blockchain && npx hardhat node",
    "contract:compile": "cd src/blockchain && npx hardhat compile",
    "contract:test": "cd src/blockchain && npx hardhat test test/*.ts",
    "contract:deploy:local": "cd src/blockchain && npx hardhat run scripts/deploy.ts --network localhost",
    "contract:deploy:sepolia": "cd src/blockchain && npx hardhat run scripts/deploy.ts --network sepolia"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
    "@types/uuid": "^10.0.0",
    "@ucans/ucans": "^0.12.0",
    "amqplib": "^0.10.5",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "did-jwt": "^8.0.8",
    "dids": "^5.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "fhir-kit-client": "^1.9.2",
    "framer-motion": "^12.4.2",
    "helmet": "^8.0.0",
    "key-did-provider-ed25519": "^4.0.2",
    "key-did-resolver": "^4.0.0",
    "openai": "^4.79.1",
    "uint8arrays": "^5.1.0",
    "uuid": "^11.0.5",
    "viem": "^2.22.17"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fhir": "^0.0.41",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
